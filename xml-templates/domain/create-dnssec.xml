<?xml version="1.0" encoding="UTF-8"?>
<!-- Domain Create - with DNSSEC DS data (RFC 5910) -->
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
  <command>
    <create>
      <domain:create xmlns:domain="urn:ietf:params:xml:ns:domain-1.0">
        <domain:name>example.ae</domain:name>
        <domain:period unit="y">1</domain:period>
        <domain:ns>
          <domain:hostObj>ns1.example.ae</domain:hostObj>
          <domain:hostObj>ns2.example.ae</domain:hostObj>
        </domain:ns>
        <domain:registrant>REGISTRANT_ID</domain:registrant>
        <domain:contact type="tech">TECH_CONTACT_ID</domain:contact>
        <domain:authInfo>
          <domain:pw>Ab12!@Cd34#$Ef56</domain:pw>
        </domain:authInfo>
      </domain:create>
    </create>
    <extension>
      <secDNS:create xmlns:secDNS="urn:ietf:params:xml:ns:secDNS-1.1">
        <secDNS:dsData>
          <secDNS:keyTag>12345</secDNS:keyTag>
          <!-- alg: 5=RSA/SHA-1, 7=RSASHA1-NSEC3, 8=RSA/SHA-256, 13=ECDSA/P-256 -->
          <secDNS:alg>8</secDNS:alg>
          <!-- digestType: 1=SHA-1, 2=SHA-256, 4=SHA-384 -->
          <secDNS:digestType>2</secDNS:digestType>
          <secDNS:digest>49FD46E6C4B45C55D4AC49FD46E6C4B45C55D4AC49FD46E6C4B45C55D4AC1234</secDNS:digest>
          <!-- Optional: keyData -->
          <!--
          <secDNS:keyData>
            <secDNS:flags>257</secDNS:flags>
            <secDNS:protocol>3</secDNS:protocol>
            <secDNS:alg>8</secDNS:alg>
            <secDNS:pubKey>BASE64_PUBLIC_KEY</secDNS:pubKey>
          </secDNS:keyData>
          -->
        </secDNS:dsData>
      </secDNS:create>
    </extension>
    <clTRID>DOM.CREATE.010</clTRID>
  </command>
</epp>
