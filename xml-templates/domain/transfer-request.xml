<?xml version="1.0" encoding="UTF-8"?>
<!-- Domain Transfer Request (gaining registrar initiates) -->
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
  <command>
    <transfer op="request">
      <domain:transfer xmlns:domain="urn:ietf:params:xml:ns:domain-1.0">
        <domain:name>example.ae</domain:name>
        <domain:authInfo>
          <domain:pw>DOMAIN_AUTH_PASSWORD</domain:pw>
        </domain:authInfo>
        <!-- Optional: renewal period on transfer -->
        <!-- <domain:period unit="y">1</domain:period> -->
      </domain:transfer>
    </transfer>
    <clTRID>DOM.TRANSFER.REQ.001</clTRID>
  </command>
</epp>
