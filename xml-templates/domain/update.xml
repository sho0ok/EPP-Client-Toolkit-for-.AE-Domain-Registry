<?xml version="1.0" encoding="UTF-8"?>
<!-- Domain Update - add/remove/change domain attributes -->
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
  <command>
    <update>
      <domain:update xmlns:domain="urn:ietf:params:xml:ns:domain-1.0">
        <domain:name>example.ae</domain:name>

        <!-- Add elements (all optional) -->
        <domain:add>
          <domain:ns>
            <domain:hostObj>ns3.example.ae</domain:hostObj>
          </domain:ns>
          <domain:contact type="tech">NEW_TECH</domain:contact>
          <!-- Status values: clientDeleteProhibited, clientHold,
               clientRenewProhibited, clientTransferProhibited,
               clientUpdateProhibited -->
          <domain:status s="clientTransferProhibited"/>
        </domain:add>

        <!-- Remove elements (all optional) -->
        <domain:rem>
          <domain:ns>
            <domain:hostObj>ns1.example.ae</domain:hostObj>
          </domain:ns>
          <domain:contact type="tech">OLD_TECH</domain:contact>
          <domain:status s="clientHold"/>
        </domain:rem>

        <!-- Change elements (all optional) -->
        <domain:chg>
          <domain:registrant>NEW_REGISTRANT_ID</domain:registrant>
          <domain:authInfo>
            <domain:pw>NewPassword!@34Ab</domain:pw>
          </domain:authInfo>
        </domain:chg>
      </domain:update>
    </update>
    <clTRID>DOM.UPDATE.001</clTRID>
  </command>
</epp>
