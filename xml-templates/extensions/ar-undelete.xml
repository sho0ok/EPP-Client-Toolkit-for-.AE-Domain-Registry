<?xml version="1.0" encoding="UTF-8"?>
<!-- AR Extension: Undelete a domain (restore from pendingDelete) -->
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
  <command>
    <update>
      <arext:update xmlns:arext="urn:X-ar:params:xml:ns:arext-1.0">
        <arext:undelete>
          <arext:domainName>example.ae</arext:domainName>
        </arext:undelete>
      </arext:update>
    </update>
    <clTRID>AR.UNDELETE.001</clTRID>
  </command>
</epp>
